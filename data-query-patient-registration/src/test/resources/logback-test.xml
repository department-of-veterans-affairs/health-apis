<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <include resource="org/springframework/boot/logging/logback/base.xml"/>
  <statusListener class="ch.qos.logback.core.status.NopStatusListener"/>
  <root level="${LOGBACK_ROOT_LEVEL:-INFO}"/>
  <logger level="INFO" name="org.apache.http.wire"/>
  <logger level="OFF" name="org.apache.http"/>
  <logger level="OFF" name="org.hibernate.validator.internal"/>
  <logger level="ERROR" name="com.amazonaws.auth.profile.internal.BasicProfileConfigLoader"/>
  <logger level="INFO" name="com.amazonaws"/>
  <logger level="WARN" name="com.amazonaws.services.dynamodbv2.local"/>
</configuration>
